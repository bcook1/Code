CREATE OR REPLACE
PROCEDURE ADD_USERSSKILLS(
    SKILL_ID     IN VARCHAR2,
    RESUMATOR_ID IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  INSERT INTO USERSSKILLS VALUES
    ( SKILL_ID, RESUMATOR_ID, DISPLAY_RANK
    );
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END;

