DROP PROCEDURE ADD_AWARDS;
DROP PROCEDURE ADD_CERTIFICATONS;
DROP PROCEDURE ADD_COMMENTS;
DROP PROCEDURE ADD_COURSE_WORK;
DROP PROCEDURE ADD_EDUCATION;
DROP PROCEDURE ADD_EX_CUR_DESC;
DROP PROCEDURE ADD_PROJ_DESC;
DROP PROCEDURE ADD_PROJECTS;
DROP PROCEDURE ADD_REFERENCES;
DROP PROCEDURE ADD_RESEARCH_PUB;
DROP PROCEDURE ADD_USERSSKILLS;
DROP PROCEDURE ADD_WORK_DESC;
DROP PROCEDURE ADD_WORK_EXPERIENCE;

CREATE OR REPLACE
PROCEDURE ADD_AWARDS(
    RESUMATOR_ID IN VARCHAR2,
    HONOR_NAME   IN VARCHAR2,
    YEAR         IN DATE,
    SEMESTER     IN VARCHAR2,
    DISPLAY_RANK IN NUMBER)
AS
BEGIN
  INSERT
  INTO AWARD VALUES
    (
      HONOR_ID_SEQ.NEXTVAL,
      RESUMATOR_ID,
      HONOR_NAME,
      YEAR,
      SEMESTER,
      DISPLAY_RANK
    );
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END;

