CREATE OR REPLACE
PROCEDURE ADD_WORK_EXPERIENCE(
    RESUMATOR_ID   IN VARCHAR2,
    START_DATE     IN DATE,
    END_DATE       IN DATE,
    CITY           IN VARCHAR2,
    STATE          IN VARCHAR2,
    EMPLOYER       IN VARCHAR2,
    POSITION_TITLE IN VARCHAR2,
    DISPLAY_RANK   IN NUMBER )
AS
BEGIN
  INSERT
  INTO WORK_EXPERIENCE VALUES
    (
      WORK_ID_SEQ.NEXTVAL,
      RESUMATOR_ID,
      START_DATE,
      END_DATE,
      CITY,
      STATE,
      EMPLOYER,
      POSITION_TITLE,
      DISPLAY_RANK
    );
EXCEPTION
WHEN OTHERS THEN
  raise_application_error(-20001,'An error was encountered - ' ||SQLCODE ||' -ERROR- ' ||SQLERRM);
END;